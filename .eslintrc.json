{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "warn",
        "comma-dangle": "off",
        "arrow-parens": ["off", "as-needed"],
        "consistent-return": "off",
        "max-len": [
			"error",
			{
				"code": 140,
				"ignoreTemplateLiterals": true
			}
		]
    }
}
